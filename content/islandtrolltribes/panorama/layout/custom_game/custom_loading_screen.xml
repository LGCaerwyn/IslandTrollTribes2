<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/custom_loading_screen.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/loading_screen.js" />
    </scripts>
    <Panel class="AddonLoadingRoot">
        <Panel id="seq">
            <Image id="background" class="SeqBg" src="file://{images}/loading_screen/background.png" />
            <Panel id="CenterTitle">
                <Image id="logo" src="file://{images}/loading_screen/logo.png" />
                <Image id="name" src="file://{images}/loading_screen/itt_text.png" />
            </Panel>
            <Label id="Tip" text=""/>

            <Panel id="Content">
                <Panel id="Changelog">
                    <Label id="ChangelogText" text="#loading_screen_change_text" html="true"/>
                </Panel>
            </Panel>
        </Panel>
  </Panel>
</root>
